{
  "family": "django_app",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE" ],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "django_app",
      "essential": true,
      "memory": 384,
      "memoryReservation": 128,
      "portMappings": [
        {
          "containerPort": 8000
        }
      ]
    }
  ]
}